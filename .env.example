APP_NAME=UPanel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

# Domain - Used by Caddy for SSL (use localhost for local dev)
DOMAIN_NAME=localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database - CHANGE THESE IN PRODUCTION
DB_CONNECTION=pgsql
DB_HOST=postgres
DB_PORT=5432
DB_DATABASE=upanel
DB_USERNAME=upanel
DB_PASSWORD=  # Required - generate with: openssl rand -base64 32

BROADCAST_CONNECTION=log
CACHE_STORE=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=database
SESSION_LIFETIME=480

# Redis - CHANGE IN PRODUCTION
REDIS_HOST=redis
REDIS_PASSWORD=  # Required - generate with: openssl rand -base64 32
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="upanel@example.com"
MAIL_FROM_NAME="${APP_NAME}"
